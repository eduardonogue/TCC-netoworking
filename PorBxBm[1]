          &lt;div class=&quot;elementor-widget-container&quot;&gt;
            &lt;h2 class=&quot;elementor-heading-title elementor-size-default&quot;&gt;O QUE VOCÊ ENCONTRA NO ECOSSISTEMA DE NETWORKING EMPRESARIAL RNE?&lt;/h2&gt;
          &lt;/div&gt;
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/div&gt;
    &lt;div class=&quot;elementor-element elementor-element-20640bf e-grid e-con-boxed wpr-particle-no wpr-jarallax-no wpr-parallax-no wpr-sticky-section-no elementor-invisible e-con e-parent&quot; data-id=&quot;20640bf&quot; data-element_type=&quot;container&quot; data-settings=&quot;{&amp;quot;background_background&amp;quot;:&amp;quot;classic&amp;quot;,&amp;quot;animation&amp;quot;:&amp;quot;slideInUp&amp;quot;}&quot;&gt;
      &lt;div class=&quot;e-con-inner&quot;&gt;
        &lt;div class=&quot;elementor-element elementor-element-2e5a7a1 elementor-view-default elementor-position-top elementor-mobile-position-top elementor-widget elementor-widget-icon-box&quot; data-id=&quot;2e5a7a1&quot; data-element_type=&quot;widget&quot; data-widget_type=&quot;icon-box.default&quot;&gt;
          &lt;div class=&quot;elementor-widget-container&quot;&gt;
            &lt;div class=&quot;elementor-icon-box-wrapper&quot;&gt;

              &lt;div class=&quot;elementor-icon-box-icon&quot;&gt;
                &lt;span class=&quot;elementor-icon elementor-animation-&quot;&gt;
                  &lt;i aria-hidden=&quot;true&quot; class=&quot;fas fa-chart-line&quot;&gt;&lt;/i&gt; &lt;/span&gt;
              &lt;/div&gt;

              &lt;div class=&quot;elementor-icon-box-content&quot;&gt;

                &lt;h3 class=&quot;elementor-icon-box-title&quot;&gt;
                  &lt;span&gt;
                    INDICAÇÃO DE POTENCIAIS CLIENTES &lt;/span&gt;
                &lt;/h3&gt;

              &lt;/div&gt;

            &lt;/div&gt;
          &lt;/div&gt;
        &lt;/div&gt;
        &lt;div class=&quot;elementor-element elementor-element-6034e13 elementor-view-default elementor-position-top elementor-mobile-position-top elementor-widget elementor-widget-icon-box&quot; data-id=&quot;6034e13&quot; data-element_type=&quot;widget&quot; data-widget_type=&quot;icon-box.default&quot;&gt;
          &lt;div class=&quot;elementor-widget-container&quot;&gt;
