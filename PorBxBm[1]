        &lt;div class=&quot;elementor-element elementor-element-ec4dd6e elementor-position-right elementor-vertical-align-middle elementor-widget elementor-widget-image-box&quot; data-id=&quot;ec4dd6e&quot; data-element_type=&quot;widget&quot; data-widget_type=&quot;image-box.default&quot;&gt;
